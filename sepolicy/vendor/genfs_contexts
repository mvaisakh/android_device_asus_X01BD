genfscon debugfs /wlan0                  u:object_r:debugfs_wlan:s0
genfscon proc /tpd_gesture               u:object_r:proc_tpd_gesture:s0

## Zenparts ##

# Earpiece gain
genfscon sysfs /sys/kernel/sound_control/earpiece_gain                                       u:object_r:sysfs_egain:s0

# Headphone gain
genfscon sysfs /sys/kernel/sound_control/headphone_gain                                      u:object_r:sysfs_hgain:s0

# Microphone gain
genfscon sysfs /sys/kernel/sound_control/mic_gain                                            u:object_r:sysfs_mgain:s0
